   "source": [
    "### Combining and Splitting"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2022-01-01 12:00:00\n",
      "2022-01-01\n",
      "12:00:00\n"
     ]
    }
   ],
   "source": [
    "from datetime import datetime, date, time\n",
    "\n",
    "d = date(2022, 1, 1)\n",
    "t = time(12, 0)\n",
    "\n",
    "# Combining date and time to a datetime\n",
    "dt = datetime.combine(d, t)\n",
    "print(dt)\n",
    "\n",
    "# Splitting a datetime again\n",
    "d, t = dt.date(), dt.time()\n",
    "print(d)\n",
    "print(t)\n"
   ]
  }
